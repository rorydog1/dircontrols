# Total Pressure
#
#% archiver 10 Monitor
#% controldesk Total Pressure readback
#
record(ai, "rga1:TOTP")
{
    field(DESC, "Total Pressure")
    field(DTYP, "asynFloat64")
    field(INP,  "@asyn(A1)TOTP")
    field(SCAN, "10 second")
    field(FLNK, "rga1:TOTPLOG")
    field(EGU,  "mbar")
    field(PREC, "3")
    field(HOPR, "0.01")
    field(LOPR,"1.0e-11")
    field(PINI, "YES")
}
#
record(calcout, "rga1:TOTPADEL")
{
    field(DESC, "deadband")
    field(CALC, "A/B")
    field(OUT,  "rga1:TOTP.ADEL")
    field(INPA, "rga1:TOTP")
    field(INPB, "20")
    field(SCAN, "10 second")
}
#
#% archiver 10 Monitor
#
record(calc, "rga1:TOTPLOG")
{
    field(DESC, "Total Pressure")
    field(EGU,  "log")
    field(CALC, "LOG(A)")
    field(INPA, "rga1:TOTP MS")
    field(HOPR, "-2")
    field(LOPR, "-11")
    field(ADEL, "0.025")
}
