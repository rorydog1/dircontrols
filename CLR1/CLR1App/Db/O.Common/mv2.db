record(calc, "rga4:STA")
{
    field(DESC, "Overall status")
    field(CALC, "((A>4)&&(A<=8)&&(B#1))?0:A")
    field(SCAN, "1 second")
    field(INPA, "rga4:HEADSTA PP MS")
    field(INPB, "rga4:FILSTA PP")
}
